<% @entries.each do |e| %>
  <% unless e.user == current_user %>
    <h1><%= e.user.name %>さんとのチャット</h1>
  <% end %>
<% end %>
    <% if @messages.present? %>
      <% @messages.each do |m| %>
        <div>
          <%= m.content %>
        </div>
      <% end %>
    <% end %>
  </div>
    <%= form_for @message do |f| %>
      <%= f.text_field :content %>
      <%= f.hidden_field :room_id, value: @room.id %>
      <%= f.submit "送信" %>
    <% end %>